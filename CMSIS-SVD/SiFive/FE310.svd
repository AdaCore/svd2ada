  <vendor>SiFive</vendor>
  <vendorID>SiFive</vendorID>
  <name>FE310</name>
  <series>Freedom Everywhere</series>
  <version></version>
  <description>E31 CPU Coreplex, high-performance, 32-bit RV32IMAC core
  </description>

  <addressUnitBits>8</addressUnitBits>
