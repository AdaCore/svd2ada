              <bitOffset>0</bitOffset>
              <bitWidth>3</bitWidth>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>INFINITE</name>
                  <description>No predicted end of burst is generated and therefore INCR bursts coming from this master cannot be broken.</description>
                  <value>0x0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SINGLE</name>
                  <description>The undefined length burst is treated as a succession of single access allowing rearbitration at each beat of the INCR burst.</description>
                  <value>0x1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>FOUR_BEAT</name>
                  <description>The undefined length burst is split into a 4-beat bursts allowing rearbitration at each 4-beat burst end.</description>
                  <value>0x2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>EIGHT_BEAT</name>
                  <description>The undefined length burst is split into 8-beat bursts allowing rearbitration at each 8-beat burst end.</description>
                  <value>0x3</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>SIXTEEN_BEAT</name>
                  <description>The undefined length burst is split into 16-beat bursts allowing rearbitration at each 16-beat burst end.</description>
                  <value>0x4</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
